<div class="navbar border-bottom bg-white">
  <div class="container">
    <a class="navbar-brand" href="/articles">Wikinimous</a>
    <%= link_to 'New article', new_article_path%>
  </div>
</div>

<div class="articles-container">
<% @articles.each do |article| %>
  <div class="article">
    <h2><%= link_to article.title, article_path(article) %></h2>
    <p><%= article.content %></p>
  </div>
<% end %>
</div>
